
.CajaDialogo {
    background-color:#FFFFFF;
    border-width: 4px;
    border-style: outset;
    border-color: Yellow;
    padding: 0px;
    width: 375px;
    font-weight: bold;
    font-style: italic;
}



.CajaDialogo div {
    margin: 7px;
    text-align: center;
}

.FondoAplicacion {
    background-color: Gray;
    filter: alpha(opacity=70);
    opacity: 0.7;
}


