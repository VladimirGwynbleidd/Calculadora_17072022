.text_gral_popup {
    margin: 20px 25px 10px 25px;
    text-align:justify;
    font-family:Arial, 'DejaVu Sans', 'Liberation Sans', Freesans, sans-serif;
    font-size:13px;
    color:#ffffff;
	font-size:12px !important;
}

.text_gral_popup a:link {
    text-decoration: underline;
    color: #ffffff;
}

.text_gral_popup a:visited {
    text-decoration: underline;
    color: #ffffff;
}

.text_gral_popup a:hover {
    color: #ffffff;
}

.text_gral_popup a:active {
    color: #ffffff;
}